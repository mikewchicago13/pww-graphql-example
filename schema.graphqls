type Query {
    hello: Response
    goodbye ( name: String! ) : Response
    bookById ( id: ID ): Book
    authorById ( id: ID ): Author
    bowling ( rolls: [Int]! ): Game
}

type Game {
    score: Int
}

type Response {
    contents: String
}

type Book {
    id: ID
    title: String
    pageCount: Int
    author: Author
    methodOnEntity ( param1: String! ): String
}

type Author {
    id: ID
    firstName: String
    lastName: String
    books: [Book]
}
